{
  "log": {
    "level": "warn",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "dns-remote",
        "address": "tls://1.1.1.1"
      }
    ]
  },
  "inbounds": [
    {
      "type": "vless",
      "tag": "vless-reality",
      "listen": "::",
      "listen_port": 443,
      "sniff": true,
      "users": [
        {
          "uuid": "{{ vless_uuid }}",
          "flow": "xtls-rprx-vision"
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "www.google.com",
        "reality": {
          "enabled": true,
          "handshake": {
            "server": "www.google.com",
            "server_port": 443
          },
          "private_key": "{{ reality_private }}",
          "short_id": [ "{{ reality_shortid }}" ],
          "max_time_difference": "1m"
        }
      }
    },
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    },
  ],
  "route": {
    "rules": [
      {
        "action": "sniff"
      },
      {
        "outbound": "direct"
      }
    ]
  }
}
